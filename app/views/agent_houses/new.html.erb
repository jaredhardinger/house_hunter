<h1>Add New House to Agent</h1>

<%= form_with url: "/agents/#{@agent.id}/houses", method: :post, local: true do |form| %>
  <p><%= form.label :address %>
  <%= form.text_field :address %></p>
  <p><%= form.label :price %>
  <%= form.text_field :price %></p>
  <p><%= form.label :for_sale %>
  <%= form.check_box :for_sale %></p>
  <p><%= form.submit "Create House" %></p>

<% end %>
